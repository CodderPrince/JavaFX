#include<stdio.h>
#include<math.h>
int main(){
	int T,x,y,a,i,j;
	//cin>>T;
	scanf("%d", &T);

	for(i=0;i<T;i++){
		//cin>>x>>y;
		scanf("%d %d", &x, &y);
		if(x>y){
			//cout<<"Invalid"<<endl;
			printf("Invalid!\n");
			break;
		}
		else{
			a=y/x;
			for(j=1;j<=a;j++){
				//cout<<x*j<<endl;
				printf("%d\n",x,j,x*j);
			}
		}
		printf("\n");
	}
	return 0;
}
